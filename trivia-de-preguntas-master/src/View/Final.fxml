<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControladorPantallaFinal">
    <VBox fx:id="BoxButtons" layoutX="75.0" layoutY="124.0" prefHeight="211.0" prefWidth="461.0" spacing="6.0">
        <padding>
            <Insets top="10.0" />
        </padding>
        <children>
            <TableView fx:id="tableView" prefHeight="281.0" prefWidth="461.0">
                <columns>
                    <TableColumn fx:id="tableColumn1" prefWidth="44.0" text="Nombre" />
                    <TableColumn fx:id="tableColumn2" minWidth="0.0" prefWidth="52.0" text="Correctas" />
                    <TableColumn fx:id="tableColumn3" prefWidth="26.0" text="Tiempo" />
                </columns>
            <columnResizePolicy>
               <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
            </columnResizePolicy>
            </TableView>
        </children>
    </VBox>
    <Label layoutX="142.0" layoutY="31.0" prefHeight="39.0" prefWidth="339.0" text="Resultados Recientes" textAlignment="CENTER">
        <font>
            <Font name="Arial Black" size="26.0" />
        </font>
    </Label>
    <TextFlow layoutX="75.0" layoutY="346.0" prefHeight="46.0" prefWidth="461.0" textAlignment="CENTER">
    <children>
        <Label fx:id="Frase" alignment="CENTER" contentDisplay="CENTER" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Arial" size="17.0" />
            </font></Label>
    </children></TextFlow>
    <Button fx:id="closeButton" layoutX="248.0" layoutY="80.0" mnemonicParsing="false" onAction="#btnVolver" prefHeight="25.0" prefWidth="115.0" text="Jugar De Nuevo!" />


</AnchorPane>
